      "end_time": 3180,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "But still, like dust, I'll rise."
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Poem",
          "tag": "Transcription"
        },
        {
