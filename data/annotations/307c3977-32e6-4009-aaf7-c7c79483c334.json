    },
    {
      "start_time": 660,
      "end_time": 960,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "Somehow made our ablutions seem other humans beings "
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Performance",
          "tag": "Tempo"
        },
        {
          "category": "Poem",
          "tag": "Transcription"
        }
