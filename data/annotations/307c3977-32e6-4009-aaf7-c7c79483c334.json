      "uuid": "ab0fc9b6-7440-446c-bfd1-b907e7e6eb67"
    },
    {
      "start_time": 480,
      "end_time": 660,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "And yet each of us has arisen "
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Performance",
          "tag": "Tempo"
        },
        {
          "category": "Poem",
          "tag": "Transcription"
        }
