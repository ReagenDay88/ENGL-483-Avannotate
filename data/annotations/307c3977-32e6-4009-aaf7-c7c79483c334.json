    },
    {
      "start_time": 1140,
      "end_time": 1380,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "It's amazing wherever that abides in the human being "
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Poem",
          "tag": "Transcription"
        }
      ],
      "uuid": "ee6c8148-333e-42a5-a85f-00374212e965"
