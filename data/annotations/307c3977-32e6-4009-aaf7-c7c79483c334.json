    {
      "start_time": 2820,
      "end_time": 3000,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "You may trod me in the very dirt "
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Poem",
          "tag": "Transcription"
        },
        {
          "category": "Performance",
          "tag": "Tempo"
        }
