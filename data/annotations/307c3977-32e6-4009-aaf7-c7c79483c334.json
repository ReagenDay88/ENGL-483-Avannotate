      "start_time": 2700,
      "end_time": 2820,
      "annotation": [
        {
          "type": "paragraph",
          "children": [
            {
              "text": "With your bitter, twisted lies "
            }
          ]
        }
      ],
      "tags": [
        {
          "category": "Performance",
          "tag": "Tempo"
        },
        {
          "category": "Poem",
          "tag": "Transcription"
